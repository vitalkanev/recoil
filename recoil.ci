		case 17218: // FD2
		case 17409: // FLI
		case 17410: // VIC
			break;
		default:
			return false;
		}
		DecodeC64Multicolor(320 - FliBugCharacters * 8, content, 0x2402 + FliBugCharacters * 8, 0x402 + FliBugCharacters, 2 + FliBugCharacters, 0);
		return true;
	}

